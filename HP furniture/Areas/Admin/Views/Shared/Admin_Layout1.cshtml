<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta http-equiv="content-language" content="vi" />
    <meta name="description" content="" />
    <meta name="keywords" content="" />

    <title>@ViewBag.title</title>
    <link rel="website icon" type="jpg" href="~/Assets/assets/img/logo1.png" />

    <!-- Tell the browser to be responsive to screen width -->
    <meta content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no" name="viewport">
    <link rel="icon" type="image/png" href="favicon.ico">
    <!-- Bootstrap 3.3.5 -->
    <link href="~/Areas/Admin/Assets/bootstrap/css/bootstrap.min.css" rel="stylesheet" />
    <!-- Font Awesome -->
    <link href="~/Areas/Admin/Assets/plugins/font-awesome/css/font-awesome.min.css" rel="stylesheet" />
    <!-- Ionicons -->
    <link rel="stylesheet" href="~/Areas/Admin/Assets/plugins/ionicons/css/ionicons.min.css">
    <!-- Theme style -->
    <link rel="stylesheet" href="~/Areas/Admin/Assets/dist/css/AdminLTE.min.css">
    <link href="~/Areas/Admin/Assets/dist/css/AdminLTE.min.css" rel="stylesheet" />
    <link rel="stylesheet" href="~/Areas/Admin/Assets/dist/css/skins/_all-skins.min.css">

    <link href="~/Assets/assets/css/responsive.css" rel="stylesheet" />
    <link href="~/Assets/assets/css/main.css" rel="stylesheet" />

    @RenderSection("style", required: false)
</head>
<body class="hold-transition skin-blue sidebar-mini">
    <div class="wrapper">
        <header class="main-header">
            <!-- Logo -->
            <a href="~/Admin/BaoCaoDoanhThu/Index" class="logo">
                <!-- mini logo for sidebar mini 50x50 pixels -->
                <span class="logo-mini"><b>A</b>LT</span>
                <!-- logo for regular state and mobile devices -->
                <span class="logo-lg"><b>Quản trị</b> HPfurniture</span>
            </a>

            <!-- Header Navbar: style can be found in header.less -->
            <nav class="navbar navbar-static-top" role="navigation">
                <!-- Sidebar toggle button-->
                <a href="#" class="sidebar-toggle" data-toggle="offcanvas" role="button">
                    <span class="sr-only">Toggle navigation</span>
                </a>
                <!-- Navbar Right Menu -->
                <div class="navbar-custom-menu">
                    <ul class="nav navbar-nav">
                        <!-- Messages: style can be found in dropdown.less-->
                        <li>
                            <a href="#">
                                <i class="fa fa-envelope-o"></i>
                                <span class="label label-success">4</span>
                            </a>
                        </li>
                        <!-- Notifications: style can be found in dropdown.less -->
                        <li>
                            <a href="#">
                                <i class="fa fa-bell-o"></i>
                                <span class="label label-warning">10</span>
                            </a>
                        </li>
                        <!-- Tasks: style can be found in dropdown.less -->
                        <li>
                            <a href="#">
                                <i class="fa fa-flag-o"></i>
                                <span class="label label-danger">9</span>
                            </a>
                        </li>
                        <!-- User Account: style can be found in dropdown.less -->
                        <li class="dropdown user user-menu">
                            <a href="#" class="dropdown-toggle" data-toggle="dropdown">
                                <img src="~/Areas/Admin/Assets/dist/img/31.jpg" class="user-image" alt="User Image">
                                <span class="hidden-xs">
                                    @{
                                        //Truy xuất đối tượng NhanVien từ Session có tên là "User"
                                        var name = (HP_furniture.Models.NhanVien)HttpContext.Current.Session["User"];
                                        <span>@name.TenNhanVien</span>
                                    }
                                </span>
                            </a>
                            <ul class="dropdown-menu">
                                <!-- User image -->
                                <li class="user-header" style="background-color: #333">
                                    <img src="~/Areas/Admin/Assets/dist/img/31.jpg" class="img-circle" alt="User Image">
                                    <p>
                                        @{
                                            <span>@name.TenNhanVien</span>
                                        }
                                        <small>Quản tị viên</small>
                                    </p>
                                </li>
                                <!-- Menu Body -->
                                <li class="user-body">
                                    @*<div class="col-xs-4 text-center">
                                            <a href="#">Người theo dõi</a>
                                        </div>
                                        <div class="col-xs-4 text-center">
                                            <a href="#">Bán hàng</a>
                                        </div>
                                        <div class="col-xs-4 text-center">
                                            <a href="#">Bạn bè</a>
                                        </div>*@
                                </li>
                                <!-- Menu Footer-->
                                <li class="user-footer">
                                    <div class="pull-left">
                                        <a href="#" class="btn btn-default btn-flat">Hồ sơ</a>
                                    </div>
                                    <div class="pull-right">
                                        <a href="~/Admin/HomeAdmin/DangXuat" class="btn btn-default btn-flat">Thoát</a>
                                    </div>
                                </li>
                            </ul>
                        </li>
                        <!-- Control Sidebar Toggle Button -->
                    </ul>
                </div>

            </nav>
        </header>
        <!-- Left side column. contains the logo and sidebar -->
        <aside class="main-sidebar" style="background-color: #333">
            <!-- sidebar: style can be found in sidebar.less -->
            <section class="sidebar">
                <!-- Sidebar user panel -->
                <div class="user-panel">
                    <div class="pull-left image">
                        <img src="~/Areas/Admin/Assets/dist/img/31.jpg" class="img-circle" alt="User Image">
                    </div>
                    <div class="pull-left info">
                        <p>
                            @{
                                <span>@name.TenNhanVien</span>
                            }
                        </p>
                        <a href="#"><i class="fa fa-circle text-success"></i> Trực tuyến</a>
                    </div>
                </div>
                <!-- search form -->
                <form action="#" method="get" class="sidebar-form">
                    <div class="input-group">
                        <input type="text" name="q" class="form-control" placeholder="Tìm kiếm...">
                        <span class="input-group-btn">
                            <button type="submit" name="search" id="search-btn" class="btn btn-flat"><i class="fa fa-search"></i></button>
                        </span>
                    </div>
                </form>
                <!-- /.search form -->
                <!-- sidebar menu: : style can be found in sidebar.less -->
                <ul class="sidebar-menu">
                    <li class="header">QUẢN TRỊ HỆ THỐNG</li>
                    <li class="active treeview">
                        <a href="#">
                            <i class="fa fa-th"></i> <span>Báo cáo doanh thu</span> <i class="fa fa-angle-left pull-right"></i>
                        </a>
                        <ul class="treeview-menu">
                            <li><a href="~/Admin/BaoCaoDoanhThu/Index"><i class="fa fa-circle-o"></i>Báo cáo</a></li>
                        </ul>
                    </li>
                    <li class="active treeview">
                        <a href="#">
                            <i class="fa fa-th"></i> <span>Quản lý sản phẩm</span> <i class="fa fa-angle-left pull-right"></i>
                        </a>
                        <ul class="treeview-menu">
                            <li><a href="~/Admin/QL_SanPham/Index"><i class="fa fa-circle-o"></i>Danh sách sản phẩm</a></li>
                            <li><a href="~/Admin/QL_ChiTietSanPham/Index"><i class="fa fa-circle-o"></i>Danh sách chi tiết sản phẩm</a></li>
                        </ul>
                    </li>
                    <li class="active treeview">
                        <a href="#">
                            <i class="fa fa-th"></i> <span>Quản lý phòng</span> <i class="fa fa-angle-left pull-right"></i>
                        </a>
                        <ul class="treeview-menu">
                            <li><a href="~/Admin/QL_Phong/Index"><i class="fa fa-circle-o"></i>Danh sách phòng</a></li>
                            <li><a href="~/Admin/QL_ChiTietPhong/Index"><i class="fa fa-circle-o"></i>Danh sách chi tiết phòng</a></li>
                        </ul>
                    </li>
                    <li class="active treeview">
                        <a href="#">
                            <i class="fa fa-th"></i> <span>Quản lý bộ sưu tập</span> <i class="fa fa-angle-left pull-right"></i>
                        </a>
                        <ul class="treeview-menu">
                            <li><a href="~/Admin/QL_BoSuuTap/Index"><i class="fa fa-circle-o"></i>Danh sách bộ sưu tập</a></li>
                        </ul>
                    </li>
                    <li class="active treeview">
                        <a href="#">
                            <i class="fa fa-th"></i> <span>Quản lý góc cảm hứng</span> <i class="fa fa-angle-left pull-right"></i>
                        </a>
                        <ul class="treeview-menu">
                            <li><a href="~/Admin/QL_GocCamHung/Index"><i class="fa fa-circle-o"></i>Danh sách góc cảm hứng</a></li>
                        </ul>
                    </li>
                    <li class="active treeview">
                        <a href="#">
                            <i class="fa fa-th"></i> <span>Quản lý hóa đơn</span> <i class="fa fa-angle-left pull-right"></i>
                        </a>
                        <ul class="treeview-menu">
                            <li><a href="~/Admin/QL_HoaDon/Index"><i class="fa fa-circle-o"></i>Danh sách hóa đơn</a></li>
                            <li><a href="~/Admin/QL_ChiTietHoaDon/Index"><i class="fa fa-circle-o"></i>Danh sách chi tiết hóa đơn</a></li>
                        </ul>
                    </li>
                    <li class="active treeview">
                        <a href="#">
                            <i class="fa fa-th"></i> <span>Quản lý nhân viên</span> <i class="fa fa-angle-left pull-right"></i>
                        </a>
                        <ul class="treeview-menu">
                            <li><a href="~/Admin/QL_NhanVien/Index"><i class="fa fa-circle-o"></i>Danh sách nhân viên</a></li>
                        </ul>
                    </li>
                    <li class="active treeview">
                        <a href="#">
                            <i class="fa fa-th"></i> <span>Quản lý khách hàng</span> <i class="fa fa-angle-left pull-right"></i>
                        </a>
                        <ul class="treeview-menu">
                            <li><a href="~/Admin/QL_KhachHang/Index"><i class="fa fa-circle-o"></i>Danh sách khách hàng</a></li>
                        </ul>
                    </li>
                    <li class="active treeview">
                        <a href="#">
                            <i class="fa fa-th"></i> <span>Thiết kế nội thật</span> <i class="fa fa-angle-left pull-right"></i>
                        </a>
                        <ul class="treeview-menu">
                            <li><a href="~/Admin/QL_ThietKeNoiThat/Index"><i class="fa fa-circle-o"></i>Yêu cầu của khách hàng</a></li>
                        </ul>
                    </li>
                    <li class="active treeview">
                        <a href="#">
                            <i class="fa fa-th"></i> <span>Hỗ trợ</span> <i class="fa fa-angle-left pull-right"></i>
                        </a>
                        <ul class="treeview-menu">
                            <li><a href="~/Admin/QL_YeuCauHoTro/Index"><i class="fa fa-circle-o"></i>Yêu cầu của khách hàng</a></li>
                        </ul>
                    </li>
                </ul>
            </section>
            <!-- /.sidebar -->
        </aside>

        <!-- Content Wrapper. Contains page content -->
        <div class="content-wrapper" style="background-color: whitesmoke">
            <!-- Content Header (Page header) -->
            <section class="content-header">
                <ol class="breadcrumb">
                    <li><a href="~/Admin/BaoCaoDoanhThu/Index"><i class="fa fa-dashboard"></i> Trang chủ</a></li>
                    <li class="active">Quản trị</li>
                </ol>
            </section>

            <!-- Main content -->
            <section class="content">
                @RenderBody()
            </section><!-- /.content -->
        </div><!-- /.content-wrapper -->

        <footer class="main-footer">
            <div class="pull-right hidden-xs">
                <b>Version</b> 2.3.0
            </div>
            <strong>Copyright &copy; 2024-2025 <a href="http://almsaeedstudio.com" style="color: grey">Almsaeed Studio</a>.</strong> All rights reserved.
        </footer>


    </div><!-- ./wrapper -->
    <!-- jQuery 2.1.4 -->
    <script src="~/Areas/Admin/Assets/plugins/jQuery/jQuery-2.1.4.min.js"></script>
    <!-- Bootstrap 3.3.5 -->
    <script src="~/Areas/Admin/Assets/bootstrap/js/bootstrap.min.js"></script>
    <!-- FastClick -->
    <script src="~/Areas/Admin/Assets/plugins/fastclick/fastclick.min.js"></script>
    <!-- AdminLTE App -->
    <script src="~/Areas/Admin/Assets/dist/js/app.min.js"></script>
    <!-- SlimScroll 1.3.0 -->
    <script src="~/Areas/Admin/Assets/plugins/slimScroll/jquery.slimscroll.min.js"></script>
    <!-- AdminLTE dashboard demo (This is only for demo purposes) -->
    <script src="~/Areas/Admin/Assets/dist/js/pages/dashboard2.js"></script>
    <!-- AdminLTE for demo purposes -->
    <script src="~/Areas/Admin/Assets/dist/js/demo.js"></script>

    <!-- CkEditor CkFinder -->
    <script src="~/Areas/Admin/Plugin/ckfinder/ckfinder.js"></script>
    <script src="~/Areas/Admin/Plugin/ckeditor/ckeditor.js"></script>
    @RenderSection("HieuJS", false)

    @RenderSection("scripts", required: false)
</body>
</html>
